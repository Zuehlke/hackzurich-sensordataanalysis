{
  "id": "sensor-ingestion",
  "instances": 1,
  "cpus":2,
  "mem":4096,
  "disk": 100,
  "container":{
    "docker":{
      "forcePullImage":true,
      "image":"mspringma1/sensor-ingestion",
      "network":"HOST",
      "privileged": false
    },
    "type":"DOCKER"
  },
  "labels":{
    "HAPROXY_GROUP":"external",
    "HAPROXY_0_PORT": "8083"
  },
  "portDefinitions": [
    {
      "port": 10099,
      "protocol": "tcp",
      "labels": {}
    },
    {
      "port": 10100,
      "protocol": "tcp",
      "labels": {}
    }
  ],
  "healthChecks": [
    {
      "protocol": "TCP",
      "path": "/hello",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 15,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    }
  ]
}
